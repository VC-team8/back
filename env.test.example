# Test Environment Variables
# Copy this file to .env.test and update values as needed

# MongoDB Test Database
# IMPORTANT: Use a separate database for testing!
MONGODB_URI=mongodb://localhost:27017/onboard-ai-test

# Application
PORT=8001
NODE_ENV=test

# JWT Secret (use different from production!)
JWT_SECRET=test-secret-key-do-not-use-in-production

# Upload settings
MAX_FILE_SIZE=10485760
UPLOAD_PATH=./test-uploads

# OpenAI (optional for tests)
# OPENAI_API_KEY=sk-your-test-key-here

